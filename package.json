{
  "name": "lit-component-example",
  "private": false,
  "version": "0.15.0",
  "type": "module",
  "main": "dist",
  "exports": {
    ".": {
      "import": "./dist/atoms/index.js",
      "require": "./dist/atoms/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "types": "dist/types/src/components/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-storybook": "storybook build",
    "storybook": "concurrently \"npm:watch:*\"",
    "watch:storybook": "storybook dev -p 6006",
    "watch:tailwind": "concurrently \"tailwindcss -i ./src/tailwindlib.css -o ./tw.css --watch\" \"twlit --input ./tw.css --output ./tw.js\""
  },
  "dependencies": {
    "@lit-labs/react": "^2.0.0",
    "@types/react": "^18.2.20",
    "lit": "^2.7.6",
    "react": "^18.2.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/blocks": "^7.2.3",
    "@storybook/web-components": "^7.2.3",
    "@storybook/web-components-vite": "^7.2.3",
    "concurrently": "^8.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.2.3",
    "twlit": "0.0.1-alpha.37",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
